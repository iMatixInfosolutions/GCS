@section PageScript{
	<script>
		var i=5;
		transferInSeconds();

		function transferInSeconds() {
			$('#transferringInSeconds').text(i--);
			if (i == 0) location.href = '/Customers';
			//setTimeout('transferInSeconds()', 1000);
		}

	</script>
}